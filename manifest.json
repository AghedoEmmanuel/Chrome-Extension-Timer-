{
  "manifest_version": 3,
  "name": "Timer Extension",
  "description": "First Chrome Extension",
  "version": "1.0.1",
  "icons": {
    "16": "ICON3.png",
    "32": "ICON3.png",
    "48": "ICON3.png",
    "128": "ICON3.png"
  },
  "action": {
    "default_icons": {
      "16": "ICON3.png",
      "32": "ICON3.png",
      "48": "ICON3.png",
      "128": "ICON3.png"
    },
    "default_title": "Timer Extension Execution",
    "default_popup":"popup.html"
  },
  "options_page":"options.html",
  "permissions":["storage", "alarms", "notifications"],
  "background":{
    "service_worker":"background.js"
  }
}
